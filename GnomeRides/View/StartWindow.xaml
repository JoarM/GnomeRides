<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:GnomeRides.View"
    x:Class="GnomeRides.View.StartWindow"
    mc:Ignorable="d"
    Title="Gnome rides" Height="600" Width="450"
    WindowStartupLocation="CenterScreen"
    Foreground="#13170E"
    Background="#F9FAF6"
    >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="32" />
            <RowDefinition />
            <RowDefinition Height="32" />
        </Grid.RowDefinitions>
        <TextBlock FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#8AAD59">Gnome rides</TextBlock>
        <DockPanel Grid.Row="1">
            <Frame x:Name="_startFrame" />
        </DockPanel>
    </Grid>
</Window>
